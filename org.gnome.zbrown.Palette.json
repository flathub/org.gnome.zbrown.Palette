{
    "app-id": "org.gnome.zbrown.Palette",
    "runtime": "org.gnome.Platform",
    "runtime-version": "3.32",
    "sdk": "org.gnome.Sdk",
    "command": "org.gnome.zbrown.Palette",
    "finish-args": [
        "--share=ipc",
        "--device=dri",
        "--socket=fallback-x11",
        "--socket=wayland",
        "--filesystem=xdg-run/dconf",
        "--filesystem=~/.config/dconf:ro",
        "--talk-name=ca.desrt.dconf",
        "--env=DCONF_USER_CONFIG_DIR=.config/dconf"
    ],
    "modules": [
        {
            "name": "palette",
            "buildsystem": "meson",
            "sources": [
                {
                    "type": "git",
                    "url": "https://gitlab.gnome.org/World/palette",
                    "tag": "0.9.5",
                    "commit": "720d675ba38aaca8b55bbc5b1674675e5afadc0a"
                }
            ]
        }
    ]
}
